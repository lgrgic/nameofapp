<dic class="container"
  <div class="row">
    <% @orders.each do |order| %>
      <div class="col-sm-2 col-md-4">
        <div class="thumbnail">
        
        <p><%= order.product.description %></p>
        <strong>$ <%= order.product.price %></strong>
        <%= image_tag(order.product.image_url, class: "img-responsive") %>
      </div>
      </div><!-- /col -->
    <% end %>
  </div>
</div>
